{
  "manifest_version": 3,
  "name": "BreakupBuddy",
  "description": "Nobody wants to see their ex",
  "version": "1.0",
  "icons": {
    "16": "BB_Icon.png",
    "48": "BB_Icon.png",
    "128": "BB_Icon.png"
  },
  "action": {
    "default_popup": "hello.html",
    "default_icon": "BB_Icon.png"
  },
  "permissions": ["activeTab", "scripting", "tabs"],
  "background": {
    "service_worker": "background.js"
  }
}
